int main(void){
    int a, b, c;

    printf("digite um numero: \n");
    scanf("%d", &a);

    printf("digite outro numero: \n");
    scanf("%d", &b);

    printf("digite um ultimo numero: \n");
    scanf("%d", &c);
    
    int maior = (a >= b && a >= c) ? a : ((b >= a && b >= c) ? b : c);

    printf("o maior número é: %d\n", maior);

    return 0;
}
