int main(void){
    int a, b, c;

    printf("digite um numero: \n");
    scanf("%d", &a);

    printf("digite outro numero: \n");
    scanf("%d", &b);

    printf("digite um ultimo numero: \n");
    scanf("%d", &c);

    if(a >= b && a >= c){
         printf("o maior numero é: %d\n", a);
        } else if (b >= a && b >= c) {
            printf("o maior numero é: %d\n", b);
        } else {
            printf("o maior numero é: %d\n", c);
        }
}
